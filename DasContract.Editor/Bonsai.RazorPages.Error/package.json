    "JavaScript",
    "TypeScript"
  ],
  "author": "Martin Drozd√≠k <info@bonsai-development.cz> (http://bonsai-development.cz)",
  "license": "MIT",
  "devDependencies": {},
  "dependencies": {
    "@drozdik.m/common-button": "^1.0.3",
    "@drozdik.m/common-mixins": "^1.0.5",
    "@drozdik.m/px-in-rem": "^1.0.1"
  }
}
