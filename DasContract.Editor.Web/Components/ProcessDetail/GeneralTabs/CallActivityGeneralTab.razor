@using DasContract.Abstraction.Processes
@using DasContract.Editor.Web.Components.Select2;

<TabSection SectionName="Details">
    <div class="mb-3">
        <label class="form-label">Called process</label>
        <Select2 TItem="string"
                 Options="ProcessModelManager.GetAllProcessIds()"
                 ContentSelector="(p) => p"
                 ValueSelector="(p) => p"
                 Id="@($"{CallActivity.Id}-called-process")"
                 Selected="@(CreateSelectedProcessList())"
                 SelectedChanged="@((selected) => {CallActivity.CalledElement = selected.FirstOrDefault(); })"
                 Multiple="false" />
    </div>
</TabSection>
